{
	"compilerOptions": {
	"strictNullChecks": true,
		"noImplicitAny": true,
		"module": "commonjs",
		"target": "es2020",
		"allowJs": false,
		"lib": ["es2020", "DOM"],
		"types": ["@citizenfx/client", "@citizenfx/server"],
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"sourceMap": true
	},
	"include": ["./**/*"],
	"exclude": ["node_modules", "**/__tests__/*"],
}