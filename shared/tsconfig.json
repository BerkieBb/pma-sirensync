{
	"compilerOptions": {
	"strictNullChecks": true,
	  "noImplicitAny": true,
	  "module": "commonjs",
	  "target": "es6",
	  "allowJs": false,
	  "lib": ["es2015", "DOM"],
	  "types": ["@citizenfx/client"],
	  "moduleResolution": "node",
	  "resolveJsonModule": true,
	  "esModuleInterop": true,
		"sourceMap": true
	},
	"include": ["./**/*"],
	"exclude": ["node_modules", "**/__tests__/*"],
  }
  
